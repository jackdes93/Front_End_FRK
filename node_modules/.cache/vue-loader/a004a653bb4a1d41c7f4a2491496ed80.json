ry(category) {\n            socket_bus.$emit('EDIT_CATEGORY', {data: category})\n        },\n        handle_remove_category(category) {\n            socket_bus.$emit('DEL_CATEGORY', {data: category, message: category.name + ' is delete ?'})\n        }\n    },\n     created() {\n        this.handle_get_categories()\n        socket_bus.$on('CANCEL_ADD_BRAND', (result) => {\n            this.is_add = result.state\n        })\n\n        socket_bus.$on('SUCCESS_DEL_CATEGORY', (result) => {\n            if(result.is_delete) {\n                this.categories.splice(this.categories.indexOf(result.data), 1)\n                HTTP.delete('/categories/' + result.data.id).then(result => {\n                    console.log(result);\n                })\n            }\n        })\n\n        socket_bus.$on('ADD_SUCCESS_CATEGORY', (result) => {\n            this.categories.push(result.data)\n        })\n     }\n    \n}\n",{"version":3,"sources":["MenuEditor.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoDA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA","file":"MenuEditor.vue","sourceRoot":"src/components/Pages/Admin/Pages","sourcesContent":["<template>\n    <div class=\"flex-box padding-full block-custom-header\">\n        <div class=\"block-custom col-12 padding-fix-lr box-shadow margin-fix-right\" style=\"max-height: 800px; background: #EDEDED\">\n            <div class=\"block-header bg-title-header padding-full header-fix\">\n                <p class=\"title-form\">Categories</p>\n            </div>\n            <div class=\"form-group row-group\" style=\"justify-content: flex-end\">\n                <button class=\"btn btn-signup primary-color\" style=\"color: #FFF; \" v-on:click=\"is_add = true\">New Category</button>\n            </div>\n            <div class=\"form-group row-group width-full\">\n                <div class=\"block-list-row width-full padding-full border-bottom-ol\">\n                    <div class=\"block-item-row-header\" style=\"max-width: 84px;\">\n                        <p class=\"label text-color-second \">No.</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second \">Category</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second \">Description</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second \">Action</p>\n                    </div>\n                </div>\n            </div>\n            <form class=\"block-form\" style=\"padding: 0 8px 16px 8px;\">\n                <div class=\"form-group row-group padding-full\" v-if=\"categories.length == 0\">\n                    <span class=\"label text-center width-full\">No Data</span>\n                </div>\n                <div class=\"form-group row-group border-bottom-ol padding-full row-animation\" v-else v-for=\"(category, index) in categories\" :key=\"index\">\n                    <div class=\"block-item-row\"  style=\"max-width: 84px;\">\n                        <p class=\"label-cell \">{{index + 1}}</p>\n                    </div>\n                    <div class=\"block-item-row\">\n                        <p class=\"label-cell \">{{category.name}}</p>\n                    </div>\n                    <div class=\"block-item-row\">\n                        <p class=\"label-cell \">{{category.description}}</p>\n                    </div>\n                   <div class=\"block-item-row\" style=\"justify-content: space-between; align-items: flex-end; flex-direction: row\">\n                        <a v-on:click=\"handle_edit_category(category)\" class=\"label-cell flex-box\"><i class=\"fas fa-edit\"></i></a>\n                        <a v-on:click=\"handle_remove_category(category)\" class=\"label-cell flex-box\"><i class=\"fas fa-trash-alt\"></i></a>\n                    </div>\n                </div>\n            </form>\n        </div>\n        <form_add_item v-if=\"is_add\"></form_add_item>\n        <form_detail></form_detail>\n    </div>\n</template>\n\n<script>\nimport {socket_bus } from '../../../../main';\nimport form_add_item from '../Pages/FormAddMenuItem';\nimport form_detail from '../Pages/FormDetailCategory'\nimport { HTTP } from '../../../../http-common';\n\nexport default {\n    components:{\n        form_add_item, form_detail\n    },\n    data() {\n        return {\n            is_add : false,\n            categories: []\n        }\n    },\n    methods: {\n        handle_get_categories() {\n            this.$store.dispatch('get_categories').then(result => {\n                this.categories = result.status == 200 ? result.data : []\n            })\n        },\n        handle_edit_category(category) {\n            socket_bus.$emit('EDIT_CATEGORY', {data: category})\n        },\n        handle_remove_category(category) {\n            socket_bus.$emit('DEL_CATEGORY', {data: category, message: category.name + ' is delete ?'})\n        }\n    },\n     created() {\n        this.handle_get_categories()\n        socket_bus.$on('CANCEL_ADD_BRAND', (result) => {\n            this.is_add = result.state\n        })\n\n        socket_bus.$on('SUCCESS_DEL_CATEGORY', (result) => {\n            if(result.is_delete) {\n                this.categories.splice(this.categories.indexOf(result.data), 1)\n                HTTP.delete('/categories/' + result.data.id).then(result => {\n                    console.log(result);\n                })\n            }\n        })\n\n        socket_bus.$on('ADD_SUCCESS_CATEGORY', (result) => {\n            this.categories.push(result.data)\n        })\n     }\n    \n}\n</script>"]}]}