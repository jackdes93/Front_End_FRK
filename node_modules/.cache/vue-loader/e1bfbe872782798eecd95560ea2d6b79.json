                <div class=\"form-group col-group margin-fix-right\">\n                        <span class=\"label-span margin-bottom\">Price</span>\n                        <p class=\"label\">{{selected_advertising_type.price.toLocaleString()}} <sup>đ</sup> </p>\n                    </div>\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span margin-bottom\">Total Price </span>\n                        <p class=\"label\">{{selected_advertising_type.price.toLocaleString()}} <sup>đ</sup></p>\n                    </div>\n                </div>\n                <div class=\"form-group row-group\">\n                    <div class=\"form-group col-group margin-fix-right\">\n                        <span class=\"label-span margin-bottom\">Advertisement Time</span>\n                        <select name=\"category\"  class=\"list-joining\" v-model=\"new_adv.advertising_time\">\n                            <option value=\"\" selected>-- Select Time  --</option>\n                            <option v-bind:value=\"item.value\" v-for=\"(item, index) in times\" :key=\"index\">{{item.name}}</option>\n                        </select>\n                    </div>\n                   <div class=\"form-group col-group margin-fix-right \">\n                        <span class=\"label-span margin-bottom\">Start Date</span>\n                        <input type=\"text\" name=\"\" class=\"border-bottom-ol\" >\n                    </div>\n                </div>\n                <div class=\"form-group row-group\">\n                   \n                </div>\n                <div class=\"form-group row-group\" style=\"justify-content: center;\">\n                    <button class=\"btn btn-cancel margin-fix-right\" style=\"color: #ED6B28; border: 1px solid #ED6B28\" v-on:click=\"handle_close()\">CANCEL</button>\n                    <button class=\"btn btn-signup primary-color\" style=\"color: #FFF\">SAVE</button>\n                </div>\n            </form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport {socket_bus } from '../../../../main'\n\nexport default {\n    data() {\n        return {\n            new_adv: {},\n            brands: [],\n            adver_type:[],\n            times:[{name: '1 weeks', value:7}, {name: '2 weeks', value: 14}, {name:'1 months', value: 30}, {name:'2 months', value: 60}, {name:'3 months', value: 90}, {name: '6 months', value: 180}],\n            brand_selected: {},\n            selected_advertising_type: {price: 0}\n        }\n    },\n    methods : {\n        handle_close() {\n            socket_bus.$emit('CLOSE_FORM_ADD', {data: false})\n        },\n    },\n    created() {\n        this.$store.dispatch('handle_get_brands').then (result => {\n            this.brands = result.status == 200 ? result.data : []\n        })\n\n        this.$store.dispatch('get_advertisement_type').then(result => {\n            this.adver_type = result.status == 200 ? result.data : []\n        })\n    }\n}\n</script>"]}]}