\n                    <button class=\"btn btn-cancel margin-fix-right\" style=\"color: #ED6B28; border: 1px solid #ED6B28\" v-on:click=\"handle_close()\">CANCEL</button>\n                    <button class=\"btn btn-signup primary-color\" style=\"color: #FFF\">SAVE</button>\n                </div>\n            </form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport {socket_bus } from '../../../../main'\nimport { HTTP } from '../../../../http-common';\n\nexport default {\n    data() {\n        return {\n            new_category: {}\n        }\n    },\n    methods : {\n        handle_close() {\n            socket_bus.$emit('CANCEL_ADD_BRAND', {state: false})\n        },\n        handle_add_category() {\n            this.$validator.validateAll()\n            if(!this.errors.any()) {\n                HTTP.post('/categories', this.new_category).then(result => {\n                    if(result.status == 201) {\n                        socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Add new Category Success', is_redirect: true, brand: result.data.data})\n                        socket_bus.$emit('ADD_SUCCESS_CATEGORY', {data: result.data.data})\n                        this.new_category = {}\n                    }\n                })\n            }\n        }\n    },\n    created() {\n        this.new_category = {}\n    }\n}\n</script>"]}]}