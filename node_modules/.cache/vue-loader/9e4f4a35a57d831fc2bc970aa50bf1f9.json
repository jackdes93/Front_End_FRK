n                     <div class=\"block-item-row-header margin-fix-right\" style=\"max-width: 56px;\">\n                        <p class=\"label text-color-second \">No.</p>\n                    </div>\n                    <div class=\"block-item-row-header\" style=\"max-width: 140px;\">\n                        <p class=\"label text-color-second \">Category</p>\n                    </div>\n                    <div class=\"block-item-row-header\" style=\"max-width: 150px;\">\n                        <p class=\"label text-color-second \">Brand Name</p>\n                    </div>\n                    <div class=\"block-item-row-header\" style=\"max-width: 240px;\">\n                        <p class=\"label text-color-second \">Range Time ADV</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second \">Start Date</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second \">Status</p>\n                    </div>\n                    <div class=\"block-item-row-header\" style=\"max-width: 140px;\">\n                        <p class=\"label text-color-second\" style=\"text-align:left ; width: 100%;\">Action</p>\n                    </div>\n                </div>\n            </div>\n           <form class=\"block-form\" style=\"padding: 0 8px 16px 8px;\">\n                <div class=\"form-group row-group padding-full\" v-if=\"adv_list.length == 0\">\n                    <span class=\"label text-center width-full\">No Data</span>\n                </div>\n                <div class=\"form-group row-group border-bottom-ol padding-full row-animation flex-row-center\" v-else v-for=\"(adv, index) in adv_list\" :key=\"index\">\n                    <div class=\"block-item-row \" style=\"max-width: 56px;\">\n                       <p class=\"label-cell\">{{index + 1}}</p>\n                    </div>\n                    <div class=\"block-item-row \" style=\"max-width: 140px;\">\n                        <p class=\"label-cell\">{{adv.category}}</p>\n                    </div>\n                    <div class=\"block-item-row \" style=\"max-width: 150px;\">\n                        <p class=\"label-cell\">{{adv.brand_name}}</p>\n                    </div>\n                    <div class=\"block-item-row \" style=\"max-width: 240px;\">\n                        <p class=\"label-cell bold\">{{adv.adversiting_time}} days</p>\n                    </div>\n                    <div class=\"block-item-row \">\n                        <p class=\"label-cell\">{{new Date(adv.start_time).toLocaleString()}}</p>\n                    </div>\n                    <div class=\"block-item-row \" style=\"max-width: 240px;\">\n                        <p class=\"label-cell bold\" v-bind:class=\"{'color-red' : adv.adversiting_status == 'STOP', 'color-green' : adv.adversiting_status == 'START', 'color-orange' : adv.adversiting_status == 'PROCESSING'}\">{{adv.adversiting_status}}</p>\n                    </div>\n                    <div class=\"block-item-row\" style=\"max-width: 140px;\">\n                        <a class=\"label-cell flex-box\" v-on:click=\"handle_detail_adv(adv)\">See more</a>\n                        <!-- <a class=\"label-cell flex-box\" v-on:click=\"handle_change_status(adv, 'PROCESSING')\" v-if=\"adv.adversiting_status != 'PROCE'\"><i class=\"far fa-check-circle\"></i></a> -->\n                    </div>\n                </div>\n            </form>\n        </div>\n        <div class=\"block-model\" style=\"background: rgba(0,0,0,.5)\" v-if=\"adv_selected\">\n            <div class=\"block-alert padding-full \">\n                <div class=\"block-header bg-title-header padding-full header-fix width-full\">\n                    <p class=\"title-form\">{{adv_selected.brand_name}} Advertisement</p>\n                </div>\n                <form class=\"block-form width-full padding-full\" @submit.prevent=\"handle_update\">\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span margin-bottom\">Status</span>\n                        <select name=\"category\"  class=\"list-joining\" v-model=\"adv_selected.adversiting_status\" v-validate=\"'required: true'\">\n                            <option value=\"\" selected>-- Select Status --</option>\n                            <option v-bind:value=\"item\" v-for=\"(item, index) in status\" :key=\"index\">{{item}}</option>\n                        </select>\n                    </div>\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span\">Category</span>\n                        <input type=\"text\" class=\"margin-bottom border-bottom-ol\" name=\"category\" v-model=\"adv_selected.category\" readonly>\n                    </div>\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span\">Advertisement Type</span>\n                        <input type=\"text\" class=\"margin-bottom border-bottom-ol\" name=\"advertising_type\" v-model=\"adv_selected.advertising_type\" readonly>\n                    </div>\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span\">Range Time (days)</span>\n                        <input type=\"text\" class=\"margin-bottom border-bottom-ol\" name=\"status\" v-model=\"adv_selected.adversiting_time\" readonly>\n                    </div>\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span\">Start Day</span>\n                        <input type=\"text\" class=\"margin-bottom border-bottom-ol\" name=\"status\" v-model=\"adv_selected.start_time\" readonly>\n                    </div>\n                    <div class=\"form-group row-group\" style=\"justify-content: center;\">\n                        <button class=\"btn btn-cancel margin-fix-right\" style=\"color: #ED6B28; border: 1px solid #ED6B28\" v-on:click=\"adv_selected = null\">CANCEL</button>\n                        <button class=\"btn btn-signup primary-color\" style=\"color: #FFF\">SAVE</button>\n                    </div>\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { HTTP } from '../../../../http-common';\nexport default {\n    data() {\n        return {\n            adv_list: [],\n            adv_selected: null,\n            status: ['START', 'STOP', 'PROCESSING', 'PAUSE', 'WAITING']\n        }\n    },\n    methods: {\n        handle_get_list_adv() {\n            return new Promise((resolve, reject) => {\n                HTTP.get('/advertisements').then(result => {\n                    resolve(result)\n                    this.adv_list = result.data.status == 200 ? result.data.data : []\n                })\n                .catch(error => {\n                    reject(error)\n                    console.log('Error: ' + error);\n                })\n            })\n        },\n        handle_detail_adv(adv) {\n            this.adv_selected = adv\n        },\n        handle_update() {\n            return new Promise((resolve, reject) => {\n                HTTP.patch('/advertisements/update-state/' + this.adv_selected.id, {status: this.adv_selected.adversiting_status}).then(result => {\n                    resolve(result)\n                    if(result.data.status == 200) {\n                        alert('Update state of advertisement successfull')\n                    } else {\n                        alert('Update state of advertisement fail')\n                    }\n                })\n                .catch(error => {\n                    reject(error)\n                    console.log(error)\n                })\n            })\n        }\n    },\n    created() {\n        this.handle_get_list_adv()\n    }\n}\n</script>"]}]}