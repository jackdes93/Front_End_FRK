\n                    <div class=\"header\">\n                        <div class=\"nav\" style=\"height: auto; padding-bottom: 0; height: 70px; flex-direction: row; justify-content: flex-start;\">\n                            <div class=\"logo\" style=\"flex: 1 !important; flex-grow:0.3\">\n                                <router-link to=\"/\"><img src=\"../../assets/image/Logo-Light.png\" alt=\"LOGO\" style=\"border-radius: 0; object-fit: fill;\"></router-link>\n                            </div>\n                            <div class=\"block-link\">\n                                <ul class=\"list\">\n                                    <li class=\"item-menu\"><router-link to=\"/\">{{$t('home')}}</router-link></li>\n                                <li class=\"item-menu\"><router-link to=\"/about\">{{$t('about')}}</router-link></li>\n                                <li class=\"item-menu\"><a href=\"http://franchise.bravecat.vn\">{{$t('news')}}</a></li>\n                                <li class=\"item-menu\"><router-link to=\"/contact\">{{$t('contact')}}</router-link></li>\n                                </ul>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"row\" id=\"nav-bar-joining\" >\n                <div class=\"wrap-container flex-box\" style=\"padding:0; flex-direction: row; align-items: center;\">\n                    <div class=\"block-category\">\n                        <ul class=\"list\">\n                           <li v-for=\"(category, index) in categories\" :key=\"index\">\n                                <a class=\"text-uppercase link_category\" :id=\"`${index}`\" v-on:click=\"handle_redirect_category(category.name, index)\">{{handle_translate(category.name)}}</a>\n                            </li>\n                        </ul>\n                    </div>\n                    <div class=\"btn btn-join-demand\">\n                        <i class=\"fas fa-bolt text-color-primarry\"></i>\n                        <p class=\"text-color-primary\">\n                            <router-link to=\"/joining-demand\">{{$t('joining_demand')}}</router-link>\n                        </p>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"wrap-container block-slogan\" style=\"padding: 0; height: 600px;\">\n            <div class=\"slogan\">\n                <span class=\"rectangle\"></span>\n                <h4 class=\"text-slogan text-uppercase\">good brand <br> good business</h4>\n            </div>\n        </div>\n    </div>\n</template>\n\n<style scoped>\n* { color: #FFF !important; border-color: #FFF !important;}\n.opacity-35 { background: rgba(0, 0, 0, 0.85); z-index: 990;}\n\n.block-category li:first-child { padding-left: 0 !important;}\n.bg-nav-join {display: flex; flex-direction: column; background: url('../../assets/image/blur-close-up-dairy-product-376464.jpg') no-repeat; background-position: center; background-size: cover; width: 100%; height: 760px;}\n.slogan { display: flex; flex-direction: row; justify-content: flex-end; align-items: center; width: 100%; height: 100%; position: relative; padding-right: 70px; animation: position-animation; animation-duration: 1s; }\n.text-slogan { text-align: right; font-weight: bold; font-size: 58px; font-family: 'Cabin'; justify-content: flex-end; align-items: center; z-index: 99;}\n.rectangle { display: block; width: 540px; height: 72px; background: linear-gradient(105deg, #F4971D, #EF4136) !important; position: absolute; z-index: 0; right: 10px;}\n\n@keyframes position-animation {\n     0% { transform: translate(-50%, 0%);}\n     85% { transform: translate(10%, 0%);}\n    100% { transform: translate(0%, 0%);}\n}\n</style>\n\n<script>\nimport { router } from '../../routers'\nimport { socket_bus } from '../../main';\nexport default {\n    data() {\n        return {\n            customer: {},\n            is_toogle : false,\n            categories: [],\n            image_banner: '',\n            name_picture: ''\n        }\n    },\n    methods: {\n        handle_logout() {\n            localStorage.clear()\n            router.push('/')\n            window.location.reload();\n        },\n        handle_translate(name) {\n            var result = ''\n            switch(name) {\n                case 'Drink':\n                    result = this.$t('drink')\n                    break;\n                case 'Dessert':\n                    result = this.$t('dessert')\n                    break;\n                case 'Vietnamese Foods':\n                    result = this.$t('vn_food')\n                    break;\n                case 'Foriegn Foods':\n                    result = this.$t('foriegn_fd')\n                    break;\n                case 'BBQ':\n                    result = this.$t('bbq')\n                    break;\n                case 'Brunch':\n                    result = this.$t('brunch')\n                    break;\n                case 'Hot Pot':\n                    result = this.$t('hotpot')\n                    break;\n            }\n            return result;\n        },\n        handle_redirect_category(category, index) {\n            socket_bus.$emit('CHANGE_CATEGORY', category)\n            router.push('/category=' + category)\n            var link_category = document.getElementsByClassName('link_category')\n            var category_selected = document.getElementById(index)\n            for(var i = 0; i < link_category.length; i ++) {\n                link_category[i].classList.remove('active')\n            }\n            category_selected.classList.add('active')\n           \n        },\n        handle_get_name_image(link) {\n            return String(link).split('/')[2].split('.')[0]\n        },\n        handle_get_image_advertisement() {\n            this.$store.dispatch('handle_get_link_image').then(result => {\n                this.image_banner = result.data[0].exhibition\n                // this.name_picture = this.handle_get_name_image(this.image_banner)\n                console.log(this.image_banner)\n            })\n        }\n    },\n    created() {\n        if(JSON.parse(localStorage.getItem('customer'))) {\n            this.customer = JSON.parse(localStorage.getItem('customer'))\n        }\n\n        this.$store.dispatch('get_categories').then(result => {\n            this.categories = result.data\n        })\n\n    }\n}\n</script>"]}]}