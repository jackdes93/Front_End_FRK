p class=\"label text-color-second \">Adv Time</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second \">Date Start</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second\">Note</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second \">Status</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second \">Actions</p>\n                    </div>\n                </div>\n            </div>\n            <form class=\"block-form\" style=\"padding: 0 8px 16px 8px;\">\n                <div class=\"form-group row-group\" v-if=\"advertisements.length == 0\">\n                    <span class=\"label-span text-center width-full\">No Data</span>\n                </div>\n                <div class=\"form-group row-group border-bottom-ol padding-full row-animation\" v-else v-for=\"(item, index) in advertisements\" :key=\"index\">\n                    <div class=\"block-item-row\" style=\"max-width: 148px;\">\n                        <p class=\"label-cell \">{{item.category}}</p>\n                    </div>\n                    <div class=\"block-item-row\">\n                        <p class=\"label-cell \">{{item.brand_name}}</p>\n                    </div>\n                    <div class=\"block-item-row\" style=\"max-width: 148px;\">\n                        <p class=\"label-cell bold\">{{item.price.toLocaleString()}} <sup>Ä‘</sup></p>\n                    </div>\n                    <div class=\"block-item-row\" style=\"max-width: 128px;\">\n                        <p class=\"label-cell\">{{item.adversiting_type}}</p>\n                    </div>\n                    <div class=\"block-item-row\" style=\"max-width: 128px;\">\n                        <p class=\"label-cell\">{{item.adversiting_time}} days</p>\n                    </div>\n                    <div class=\"block-item-row\">\n                        <p class=\"label-cell\">{{new Date(item.start_time).toLocaleString()}}</p>\n                    </div>\n                    <div class=\"block-item-row\">\n                        <p class=\"label-cell\">{{item.note}}</p>\n                    </div>\n                    <div class=\"block-item-row\" style=\"max-width: 128px;\">\n                        <p class=\"label-cell\">{{item.adversiting_status}}</p>\n                    </div>\n                   <div class=\"block-item-row\" style=\"justify-content: flex-end; align-items: flex-end; flex-direction: row\">\n                        <a v-on:click=\"handle_edit(item)\" v-if=\"item.adversiting_status != 'START'\" class=\"label-cell flex-box\"><i class=\"fas fa-edit\"></i></a>\n                        <a v-on:click=\"handle_lock(item)\" class=\"label-cell flex-box\"><i class=\"fas fa-lock\"></i></a>\n                        <a v-on:click=\"handle_delete(item)\" class=\"label-cell flex-box\"><i class=\"fas fa-trash-alt\"></i></a>\n                    </div>\n                </div>\n            </form>\n        </div>\n        <Form_Advertisment v-if=\"is_add\"></Form_Advertisment>\n        <div class=\"block-model\" style=\"background: rgba(0,0,0,.5)\" v-if=\"is_edit\">\n            <div class=\"block-alert padding-full\">\n                <div class=\"block-header bg-title-header padding-full header-fix width-full\">\n                    <p class=\"title-form\">Detail Advertisement For {{advertisement_selected.brand_name}} Brand</p>\n                </div>\n                <form class=\"block-form\" style=\"padding: 8px 16px!important; width: 85%;\" @submit.prevent=\"handle_save\">\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span margin-bottom\">Advertisement Type</span>\n                        <input class=\"border-bottom-ol\" v-model=\"advertisement_selected.adversiting_type\" readonly/>\n                    </div>\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span margin-bottom\">Advertisement Time (days)</span>\n                        <input class=\"border-bottom-ol\" v-model=\"advertisement_selected.adversiting_time\" readonly/>\n                    </div>\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span margin-bottom\">Note</span>\n                        <p>{{advertisement_selected.note == null ? 'No Data' : advertisement_selected.note}}</p>\n                    </div>\n                    <div class=\"form-group col-group margin-fix-right\">\n                        <span class=\"label-span margin-bottom\">Banner</span>\n                        <div class=\"form-group margin-bottom\" v-if=\"url_preview\">\n                            <span class=\"label-span\" v-on:click=\"handle_delete_link()\"><i class=\"fas fa-times-circle\"></i></span>\n                            <img v-bind:src=\"url_preview\" :alt=\"banner.name\" style=\"width: 120px; height: 120px; object-fit: contain;\">\n                        </div>\n                        <input type=\"file\" v-validate=\"`required:true`\" name=\"banner\" ref=\"banner\" v-on:change=\"handleFileUpload\" title=\"THIS SS\" placeholder=\"Chosen Your Image Banner\">\n                        <span class=\"error-text margin-fix-top\" v-show=\"errors.has('banner')\">{{errors.first('banner')}}</span>\n                    </div>\n                    <div class=\"form-group row-group\" style=\"justify-content: center'; margin-top: 10px;\">\n                        <div class=\"btn btn-cancel margin-fix-right\" style=\"color: #ED6B28; border: 1px solid #ED6B28\" v-on:click=\"is_edit = false\">CANCEL</div>\n                        <button type=\"submit\" class=\"btn btn-signup primary-color\" style=\"color: #FFF\">SAVE</button>\n                    </div>\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Form_Advertisment from './CreateNewAdvertisement'\nimport {socket_bus } from '../../../../main'\nimport { HTTP } from '../../../../http-common';\n\nexport default {\n    components:{\n        Form_Advertisment\n    },\n    data() {\n        return {\n            is_add : false,\n            is_edit: false,\n            customer: {},\n            advertisements: [],\n            advertisement_selected: {},\n            banner: '',\n            url_preview: null\n        }\n    },\n    methods: {\n        handle_load_advertisement() {\n            return new Promise((resolve, reject) => {\n                HTTP.get('/advertisements/advertisement-customer/' + this.customer.id).then(result => {\n                    resolve(result)\n                    this.advertisements = result.data.status == 200 ? result.data.data : []\n                })\n                .catch(error => {\n                    reject(error)\n                    console.log('Error: ' + error);\n                })\n            })\n        },\n        handleFileUpload(e) {\n            var files = e.target.files || e.dataTransfer.files;\n            if (!files.length)\n                return;\n            this.banner = this.$refs.banner.files[0]\n            this.url_preview = URL.createObjectURL(files[0]);\n            console.log(this.url_preview)\n        },\n        handle_delete_link() {\n            this.url_preview = null,\n            this.banner = ''\n            this.$refs.banner = []\n            console.log(this.$refs.banner)\n        },\n        handle_edit(item) {\n            this.is_edit = true\n            this.advertisement_selected = item\n        },\n        handle_save() {\n\n        },\n        handle_lock(item) {\n\n        },\n        handle_delete(item) {\n\n        }\n    },\n     created() {\n        if(JSON.parse(localStorage.getItem('customer'))) {\n            this.customer = JSON.parse(localStorage.getItem('customer'))\n        }\n        socket_bus.$on('CLOSE_FORM_ADD', (result) => {\n            this.is_add = result.state\n        })\n\n        this.handle_load_advertisement()\n\n        socket_bus.$on('CLOSE_ADD_ADV', () => {\n            this.is_add = false\n            this.handle_load_advertisement()\n        })\n     }\n    \n}\n</script>"]}]}