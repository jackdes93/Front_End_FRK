: '',\n            thumbnail: ''\n\n        }\n    },\n    methods : {\n        handle_close() {\n            socket_bus.$emit('CANCEL_ADD_BRAND', {state: false})\n        },\n        handleFileUpload(e) {\n            var files = e.target.files || e.dataTransfer.files;\n            if (!files.length)\n                return;\n            this.images = this.$refs.logo.files[0]\n            this.thumbnail = this.$refs.thumbnail.files[0]\n        },\n        handle_save() {\n           \n            const data_form = new FormData()\n            data_form.append('id_customer', this.customer.id)\n            data_form.append('id_category', this.new_brand.id_category)\n            data_form.append('name', this.new_brand.name)\n            data_form.append('phone_number', this.new_brand.phone_number)\n            data_form.append('location', this.new_brand.location.split(','))\n            data_form.append('introduce', this.new_brand.introduce)\n            data_form.append('info', JSON.stringify(this.new_brand.info))\n            data_form.append('price', this.new_brand.price)\n            data_form.append('logo', this.images)\n            data_form.append('thumbnail', this.thumbnail)\n            this.$validator.validateAll()\n            if(!this.errors.any()) {\n                HTTP.post('/brands', data_form,{\n                    headers: {\n                        'Content-Type' : 'multipart/form-data'\n                    }\n                }).then(result => {\n                    if(result.data.status == 201) {\n                            socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Create New Brand Success', is_redirect: true})\n                            socket_bus.$emit('UPDATE_LIST_BRAND')\n                        } else {\n                            socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Create New Brand Fail!', is_redirect: false})\n                        }\n                })\n                .catch(error => {\n            