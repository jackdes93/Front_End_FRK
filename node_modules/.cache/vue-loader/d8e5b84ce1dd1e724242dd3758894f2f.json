n                    </select>\n                </div>\n                <div class=\"form-group col-group\">\n                    <span class=\"label-span margin-bottom\">Name Brand</span>\n                    <input type=\"text\" class=\"border-bottom-ol margin-bottom\" v-model=\"new_brand.name\" name=\"name\" v-validate=\"`required:true`\">\n                    <span class=\"error-text margin-fix-top\" v-show=\"errors.has('name')\">{{errors.first('name')}}</span>\n                </div>\n                <div class=\"form-group row-group\">\n                    <div class=\"form-group col-group margin-fix-right\">\n                        <span class=\"label-span margin-bottom\">Logo Brand</span>\n                        <input type=\"file\" v-validate=\"`required:true`\" name=\"logo\" ref=\"logo\" v-on:change=\"handleFileUpload\">\n                        <span class=\"error-text margin-fix-top\" v-show=\"errors.has('logo')\">{{errors.first('logo')}}</span>\n                    </div>\n                    <div class=\"form-group col-group margin-fix-right\">\n                        <span class=\"label-span margin-bottom\">Thumbnail Brand</span>\n                        <input type=\"file\" v-validate=\"`required:true`\" name=\"thumbnail\" ref=\"thumbnail\" v-on:change=\"handleFileUpload\">\n                        <span class=\"error-text margin-fix-top\" v-show=\"errors.has('thumbnail')\">{{errors.first('thumbnail')}}</span>\n                    </div>\n                </div>\n                <div class=\"form-group row-group\">\n                    <div class=\"form-group col-group margin-fix-right\">\n                        <span class=\"label-span margin-bottom\">Phone Number</span>\n                        <input type=\"text\" name=\"phone_number\" v-model=\"new_brand.phone_number\" class=\"border-bottom-ol\" v-validate=\"`required: true|regex:^([0-9]+)$|max:10`\">\n                        <span class=\"error-text margin-fix-top\" v-show=\"errors.has('phone_number')\">{{errors.first('phone_number')}}</span>\n                    </div>\n                    <div class=\"form-group col-group\">\n                        <span class=\"label-span margin-bottom\">Locations</span>\n                        <input type=\"text\" name=\"location\" v-model=\"new_brand.location\" class=\"border-bottom-ol\" v-validate=\"`required: true`\">\n                        <span class=\"error-text margin-fix-top\" v-show=\"errors.has('location')\">{{errors.first('location')}}</span>\n                    </div>\n                </div>\n                <div class=\"form-group col-group \">\n                    <span class=\"label-span margin-bottom\">Website</span>\n                    <input type=\"text\" name=\"website\" v-model=\"new_brand.info.website\" class=\"border-bottom-ol\" v-validate=\"`required: true`\">\n                    <span class=\"error-text margin-fix-top\" v-show=\"errors.has('website')\">{{errors.first('website')}}</span>\n                </div>\n                <div class=\"form-group col-group\">\n                    <span class=\"label-span margin-bottom\">Price</span>\n                    <input type=\"number\" name=\"price\" v-model=\"new_brand.price\" class=\"border-bottom-ol\" v-validate=\"`required: true`\">\n                    <span class=\"error-text margin-fix-top\" v-show=\"errors.has('price')\">{{errors.first('price')}}</span>\n                </div>\n                 <div class=\"form-group col-group\">\n                    <span class=\"label-span margin-bottom\">About Brand</span>\n                    <textarea name=\"introduce\" v-model=\"new_brand.introduce\" v-validate=\"`required:true`\" cols=\"30\" rows=\"5\" placeholder=\"Type there some thing\" class=\"padding-full\" style=\"max-height: 120px\"></textarea>\n                </div>\n                <div class=\"form-group row-group\" style=\"justify-content: center; margin-top: 8px;\">\n                    <div class=\"btn btn-cancel margin-fix-right\" style=\"color: #ED6B28; border: 1px solid #ED6B28\" v-on:click=\"handle_close()\">CANCEL</div>\n                    <button class=\"btn btn-signup primary-color\" style=\"color: #FFF\" >SAVE</button>\n                </div>\n            </form>\n        </div>\n        <block_alert></block_alert>\n    </div>\n</template>\n\n<script>\nimport { socket_bus } from '../../../../main'\nimport {HTTP} from '../../../../http-common'\nimport block_alert from '../../../Units/Block_Alert'\n\nexport default {\n    components:{ block_alert},\n    data() {\n        return {\n            customer: '',\n            categories: [],\n            category_selected: '',\n            new_brand: {\n                id_category: '',\n                id_customer: '',\n                logo:'',\n                name: '',\n                phone_number: '',\n                location: [],\n                introduce: '',\n                info: {\n                    website: ''\n                },\n                price: 0\n            },\n            images: '',\n            thumbnail: ''\n\n        }\n    },\n    methods : {\n        handle_close() {\n            socket_bus.$emit('CANCEL_ADD_BRAND', {state: false})\n        },\n        handleFileUpload(e) {\n            var files = e.target.files || e.dataTransfer.files;\n            if (!files.length)\n                return;\n            this.images = this.$refs.logo.files[0]\n            this.thumbnail = this.$refs.thumbnail.files[0]\n        },\n        handle_save() {\n           \n            const data_form = new FormData()\n            data_form.append('id_customer', this.customer.id)\n            data_form.append('id_category', this.new_brand.id_category)\n            data_form.append('name', this.new_brand.name)\n            data_form.append('phone_number', this.new_brand.phone_number)\n            data_form.append('location', this.new_brand.location.split(','))\n            data_form.append('introduce', this.new_brand.introduce)\n            data_form.append('info', JSON.stringify(this.new_brand.info))\n            data_form.append('price', this.new_brand.price)\n            data_form.append('logo', this.images)\n            data_form.append('thumbnail', this.thumbnail)\n            this.$validator.validateAll()\n            if(!this.errors.any()) {\n                HTTP.post('/brands', data_form,{\n                    headers: {\n                        'Content-Type' : 'multipart/form-data'\n                    }\n                }).then(result => {\n                    if(result.data.status == 201) {\n                            socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Create New Brand Success', is_redirect: true})\n                            socket_bus.$emit('UPDATE_LIST_BRAND')\n                        } else {\n                            socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Create New Brand Fail!', is_redirect: false})\n                        }\n                })\n                .catch(error => {\n                    console.log(error);\n                })\n            }\n        }\n    },\n    created() {\n        this.$store.dispatch('get_categories').then(result => {\n            this.categories = result.data\n        })\n\n        if(JSON.parse(localStorage.getItem('customer'))) {\n            this.customer = JSON.parse(localStorage.getItem('customer'))\n        }\n    }\n}\n</script>"]}]}