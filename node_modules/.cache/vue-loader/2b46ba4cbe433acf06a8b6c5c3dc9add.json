.append('id_category', this.new_brand.id_category)\n            data_form.append('name', this.new_brand.name)\n            data_form.append('phone_number', this.new_brand.phone_number)\n            data_form.append('email', this.new_brand.email)\n            data_form.append('website', this.new_brand.website)\n            data_form.append('location', this.new_brand.location)\n            data_form.append('introduce', this.new_brand.introduce)\n            data_form.append('feature', this.new_brand.feature)\n            data_form.append('events', this.new_brand.events)\n            data_form.append('terms', this.new_brand.terms)\n            data_form.append('price', this.new_brand.price)\n            data_form.append('logo', this.images)\n            data_form.append('thumbnail', this.thumbnail)\n            for(let i = 0; i < this.main_banner.length; i++) {\n                data_form.append('main_banner', this.main_banner[i])\n            }\n\n           \n            data_form.append('image_intro', this.image_intro)\n            // this.$validator.validateAll()\n            // if(!this.errors.any()) {\n                HTTP.post('/brands', data_form,{\n                    headers: {\n                        'Content-Type' : 'multipart/form-data'\n                    }\n                }).then(result => {\n                    if(result.data.status == 201) {\n                        socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Create New Brand Success', is_redirect: true, brand: result.data.data})\n                        socket_bus.$emit('UPDATE_LIST_BRAND', {data: result.data.data})\n                    } else {\n                        socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Create New Brand Fail!', is_redirect: false})\n                    }\n                })\n                .catch(error => {\n                    console.log(error);\n                })\n            // }\n        }\n    },\n    created() {\n        this.$store.dispatch('get_categories').then(result => {\n            this.categories = result.data\n        })\n\n        if(JSON.parse(localStorage.getItem('customer'))) {\n            this.customer = JSON.parse(localStorage.getItem('customer'))\n        }\n    }\n}\n</script>"]}]}