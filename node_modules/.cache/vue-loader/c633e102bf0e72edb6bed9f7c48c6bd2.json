            </div>\n                    <div class=\"form-content\" style=\"padding: 0;  margin-top: 8px; heigh: auto\">\n                        <div class=\"form-group margin-bottom col-group\"> \n                            <div class=\"form-group\">\n                                <input type=\"text\" placeholder=\"Your name\" name=\"name\" v-model=\"name\" v-validate=\"'required:true'\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('name')\">{{errors.first('name')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom col-group\"> \n                            <div class=\"form-group\">\n                                <input type=\"text\" placeholder=\"Phone number\" name=\"phone_number\" v-model=\"phone_number\" v-validate=\"`required: true|regex:^([0-9]+)$`\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('phone_number')\">{{errors.first('phone_number')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom col-group\">\n                            <div class=\"form-group\">\n                                <input type=\"text\" name=\"address\" placeholder=\"Address\" v-model=\"address\" v-validate=\"`required: true`\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('address')\">{{errors.first('address')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom col-group\">\n                            <div class=\"form-group\">\n                                <input type=\"text\" name=\"email\" placeholder=\"Email\" v-model=\"email\" v-validate=\"'required:true|email'\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('email')\">{{errors.first('email')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom col-group\">\n                            <div class=\"form-group \">\n                                <input type=\"password\" placeholder=\"Type your password\" name=\"password\" v-model=\"password\" v-validate=\"'required:true|min:9'\" ref=\"password\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('password')\">{{errors.first('password')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom col-group\">\n                             <div class=\"form-group \">\n                                <input type=\"password\" placeholder=\"Type your password\" name=\"re_password\" v-validate=\"'required:true|confirmed:password'\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('re_password')\">{{errors.first('re_password')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom\" style=\"flex-direction:row; align-items: center;\">\n                            <input type=\"checkbox\" name=\"agree\" v-model=\"agree\" style=\"width: 40px; height: 40px;\"  v-validate=\"`required:true`\">\n                            <p style=\"font-family: 'Quicksand'; font-size: 12px; flex: 8; text-align: center; letter-spacing: 1px;\">I have read and understand the <span class=\"text-color-primarry \">terms of service</span> and <span class=\"text-color-primarry\"> primacy policy</span></p>\n                        </div>\n                        <div class=\"form-group flex-row-center\" style=\"padding: 10px 0 20px 0;\">\n                            <router-link to=\"/login\">I have account !!</router-link>\n                        </div>\n                        <div class=\"form-group\">\n                            <button class=\"btn btn-sign-up\" type=\"submit\" :disabled=\"!agree\">Register</button>   \n                        </div>\n                    </div>\n                </form>\n            </div>\n        </div>\n        <block_alert></block_alert>\n        <block_footer></block_footer>\n    </div>\n    \n</template>\n\n<style scoped>\ninput[type=\"text\"], input[type=\"password\"], input[type=\"email\"] { height: 42px;}\n</style>\n\n<script>\nimport block_nav from '../Units/BlockNav';\nimport block_footer from '../Units/Block_Footer';\nimport block_alert from '../Units/Block_Alert';\nimport { socket_bus } from '../../main'\nimport  { router } from '../../routers';\nimport { HTTP } from '../../http-common';\n \nexport default {\n    components: {\n        block_nav, block_footer, block_alert\n    },\n    data() {\n        return {\n            email: '',\n            name: '',\n            phone_number: '',\n            address: '',\n            password: '',\n            agree: false\n        }\n    },\n    methods: {\n        register() {\n            const new_customer = {\n                email: this.email,\n                password: this.password,\n                full_name: this.name,\n                address: this.address,\n                phone_number: this.phone_number\n            }\n            this.$validator.validateAll()\n            if(!this.errors.any()) {\n                HTTP.post('customers', new_customer).then(result => {\n                     if(result.data.status == 201) {\n                        socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Register Account Success', is_redirect: true})\n                    } else {\n                        socket_bus.$emit('SHOW_ALERT', {is_show: true, message: 'Register Account Fail!', is_redirect: false})\n                    }\n                })\n                .catch(error => {\n                   console.log(error)\n                })\n            }\n    },\n    created() {\n        socket_bus.$on('CLOSE_BOX', result => {\n            if(result.is_redirect) {\n                router.push('/login')\n            }\n        })\n\n        if(localStorage.getItem('access_token') != undefined) {\n            router.push('/user/info-user')\n        }\n    }\n    }\n}\n</script>"]}]}