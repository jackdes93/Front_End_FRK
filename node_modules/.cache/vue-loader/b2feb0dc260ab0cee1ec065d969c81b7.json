 <router-link :to=\"{path: `/brand/${brand.id}`}\" style=\"color:#000; width: 100%;\"><i class=\"fas fa-eye\"></i></router-link>\n                        </span>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<style scoped>\n.fa-heart, .fa-eye, .fa-phone { color: #FFF;}\n.bg-drink { background: url('../../assets/image/Drink.jpg') no-repeat; background-position: center; background-size: cover; display: flex; flex-direction: row; width: 100%; height: 790px; padding: 60px 55px;}\n.block-drink-left, .block-drink-brands { display: flex; height: 100%;}\n.block-drink-left { flex: 1;; margin-right: 15px; display: flex; flex-direction: column; justify-content: space-between; align-items: center; padding-top: 15px;}\n.block-drink-brands { flex: 5;margin-left: 15px; display: flex; flex-direction: row; width: 100%; flex-wrap: wrap;}\n.title-block-drink {color: #FFF; font-weight: bold; font-size: 32px; font-family: 'Cabin Medium' }\n.title-see-more { font-family: 'Quicksand'; font-size: 14px; color: #ed6b28}\n.item-brands { display: flex; flex-direction: column; justify-content: center; align-items: center; background: #E5E5E5 !important; height: 200px; width: 30%; border-radius: 20px; padding: 0 10px 10px 10px;}\n.item-brands:hover {background: linear-gradient(105deg, #F4971D, #EF4136) !important; justify-content: space-between; align-items: center;}\n.icon-normal { color: #CCC; font-size: 150px;}\n.item-brands:hover .icon-normal { display: none;}\n.item-brands:hover .content-brands, .item-brands:hover .block-contact { display: flex;}\n.item-brands:not(:nth-child(3n)) { margin-right: 35px; margin-bottom: 35px;}\n.content-brands { padding: 30px 30px 0px 30px; display: none; flex-direction: column; width: 100%;}\n.title-brand { font-family: 'Cabin'; font-weight: bold; font-size: 18px; color: #fff; margin-bottom: 5px;}\n.description-brand { font-family: 'Quicksand Light'; font-size: 12px; color: #fff; width: 100%; height: 44px; overflow: hidden;}\n.price-brand { font-family: 'Cabin'; font-weight: normal; font-size: 18px; color: #fff; margin-top: 10px;}\n.block-contact { display: none; width: 100%; height: 40px; flex-direction: row; justify-content: space-between; align-items: center; background: rgba(0, 0, 0, .25); border-radius: 20px; padding: 0 10px; margin-top: 10px;}\n.block-contact span:first { display: flex; flex-direction: row; width: 50%; height: 100%; justify-content: space-around; align-items: center;}\n.fa-heart { margin-right: 10px;}\n</style>\n\n<script>\nimport { HTTP } from '../../http-common';\nimport { router } from '../../routers';\n\nexport default {\n    data() {\n        return {\n            brands: [],\n        }\n    },\n    methods: {\n        handle_click() {\n\n        },\n        handle_get_brand() {\n             return new Promise((resolve, reject) => {\n                HTTP.get('brands/category/Drink').then(result => {\n                    resolve(result)\n                    this.brands = result.data.status == 200 ? result.data.data : []\n                })\n                .catch(error => {\n                    reject(error)\n                    console.log(error)\n                })\n            })\n        },\n        handle_add_wish_list(id_brand, event) {\n            if(localStorage.getItem('access_token')) {\n                this.$store.dispatch('handle_add_wish_list', {id_brand: id_brand, id_customer: this.customer.id})\n                this.$store.dispatch('handle_update_brand_into_wish_list_customer', {id_brand: id_brand, id_customer: this.customer.id})\n                const object = event.target.parentElement.classList\n                object[0] == 'wish' ? object.add('wish-hover') : object.add('')\n            } else {\n                router.push('/login')\n            }\n        },\n    },\n    created() {\n        this.handle_get_brand()\n    }\n}\n</script>"]}]}