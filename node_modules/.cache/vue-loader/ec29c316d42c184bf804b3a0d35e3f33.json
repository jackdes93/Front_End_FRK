ss=\"form-group row-group\" style=\"justify-content: flex-end\">\n                <button class=\"btn btn-signup primary-color\" style=\"color: #FFF; \" v-on:click=\"is_add = true\">New Account</button>\n            </div>\n            <div class=\"form-group row-group width-full\">\n                <div class=\"block-list-row width-full padding-full border-bottom-ol\">\n                    <div class=\"block-item-row-header margin-fix-right\" style=\"max-width: 36px; width: 36px\">\n                        <p class=\"label text-color-second \">No</p>\n                    </div>\n                    <div class=\"block-item-row-header margin-fix-right\" style=\"max-width: 140px; width: 140px;\">\n                        <p class=\"label text-color-second \">Name</p>\n                    </div>\n                    <div class=\"block-item-row-header margin-fix-right\" >\n                        <p class=\"label text-color-second \">Email</p>\n                    </div>\n                    <div class=\"block-item-row-header margin-fix-right\" style=\"max-width:140px; width: 140px;\">\n                        <p class=\"label text-color-second \">Role</p>\n                    </div>\n                    <div class=\"block-item-row-header\">\n                        <p class=\"label text-color-second\" style=\"text-align:center; width: 100%;\">Action</p>\n                    </div>\n                </div>\n            </div>\n            <form class=\"block-form\" style=\"padding: 0 8px 16px 8px;\">\n                <div class=\"form-group row-group border-bottom-ol padding-full row-animation flex-row-center\" v-for=\"(account, index) in accounts\" :key=\"index\">\n                    <div class=\"block-item-row margin-fix-right\" style=\"max-width: 36px; width: 36px\">\n                        <p class=\"label-cell\">{{index + 1}}</p>\n                    </div>\n                    <div class=\"block-item-row margin-fix-right\" style=\"max-width: 140px; width: 140px;\">\n                        <p class=\"label-cell\">{{account.full_name}}</p>\n                    </div>\n                    <div class=\"block-item-row margin-fix-right\" >\n                        <p class=\"label-cell bold\">{{account.user_name}}</p>\n                    </div>\n                    <div class=\"block-item-row margin-fix-right\" style=\"max-width:140px; width: 140px;\">\n                        <p class=\"label-cell\">{{account.roles}}</p>\n                    </div>\n                    <div class=\"block-item-row\" style=\"justify-content: flex-end; align-items: flex-end; flex-direction: row\">\n                        <a class=\"label-cell flex-box margin-fix-right md-size border-right\" style=\"justify-content: center;\" v-on:click=\"handle_reset_pass(account)\">Reset Pass</a>\n                        <a class=\"label-cell flex-box margin-fix-right md-size \" v-bind:class=\"{'border-right' : account.roles != 'ADMIN'}\" style=\"justify-content: center;\" v-on:click=\"handle_edit_account(account)\">Edit</a>\n                        <a class=\"label-cell flex-box md-size\" style=\"justify-content: center;\" v-if=\"account.roles != 'ADMIN'\"  v-on:click=\"handle_remove_account(account)\">Remove</a>\n                    </div>\n                </div>\n            </form>\n        </div>\n        <Form_Add_Account v-if=\"is_add\"></Form_Add_Account>\n        <Detail_Account></Detail_Account>\n        <div class=\"block-model\" style=\"background: rgba(0,0,0,.5)\" v-if=\"is_change_pass\">\n            <div class=\"block-alert padding-full\">\n                <div class=\"block-header bg-title-header padding-full header-fix width-full\">\n                    <p class=\"title-form\">Change Password</p>\n                </div>\n                <form class=\"block-form\" @submit.prevent=\"handle_change_password\">\n                    <div class=\"form-group margin-bottom col-group\">\n                        <span class=\"label-span margin-bottom\">Password </span>\n                        <div class=\"form-group \">\n                            <input type=\"password\" placeholder=\"Type your password\" name=\"password\" v-model=\"new_password\" v-validate=\"'required:true|min:9'\" ref=\"password\">\n                        </div>\n                        <span class=\"error-text margin-fix-top\" v-show=\"errors.has('password')\">{{errors.first('password')}}</span>\n                    </div>\n                    <div class=\"form-group margin-bottom col-group\">\n                        <span class=\"label-span margin-bottom\">Confirm Password </span>\n                        <div class=\"form-group \">\n                            <input type=\"password\" placeholder=\"Type your password\" name=\"re_password\" v-validate=\"'required:true|confirmed:password'\">\n                        </div>\n                        <span class=\"error-text margin-fix-top\" v-show=\"errors.has('re_password')\">{{errors.first('re_password')}}</span>\n                    </div>\n                    <div class=\"form-group row-group\" style=\"justify-content: center;\">\n                        <button class=\"btn btn-cancel margin-fix-right\" style=\"color: #ED6B28; border: 1px solid #ED6B28\" v-on:click=\"is_change_pass = false\">CANCEL</button>\n                        <button class=\"btn btn-signup primary-color\" style=\"color: #FFF\">SAVE</button>\n                    </div>\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n<style scoped>\n.border-right { border-right: 1px solid #A2A2A2;}\n</style>\n\n<script>\nimport {HTTP_ADMIN} from '../../../../http-common'\nimport { socket_bus } from '../../../../main';\nimport Detail_Account from '../../Admin/Pages/DetailAccountManager';\nimport Form_Add_Account from '../../Admin/Pages/FormAddAccount'\nimport { Promise } from 'q';\n\nexport default {\n    components:{\n        Detail_Account, Form_Add_Account\n    },\n    data() {\n        return 