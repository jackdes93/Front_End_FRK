n    <div>\n        <div class=\"row footer\">\n            <div class=\"block footer-top\">\n                <div class=\"block-cust\">\n                <div class=\"block introduce-brand\">\n                    <div class=\"title-brand\">\n                    <h1> Franchise King</h1>\n                    </div>\n                    <div class=\"content-introduce\">\n                    Lorem ipsum dolor sit amet consectetur adipisicing elit. Ut ea incidunt nostrum, at ipsum id? Quam, quidem quasi. Incidunt dolorum inventore tenetur delectus. Tempore at obcaecati quam placeat qui repellendus?\n                    </div>\n                </div>\n                <div class=\"block link-maps\">\n                    <div class=\"col\">\n                    <ul>\n                        <li class=\"item\">{{$t('footer.about_us')}}</li>\n                        <li class=\"item\"> {{$t('footer.plan_pricing')}}</li>\n                    </ul>\n                    </div>\n                    <div class=\"col\">\n                    <ul>\n                        <li class=\"item\">{{$t('footer.terms_condition')}}</li>\n                        <li class=\"item\">{{$t('footer.privacy_policy')}}</li>\n                        <li class=\"item\">{{$t('footer.support')}}</li>\n                    </ul>\n                    </div>\n                    <div class=\"col\">\n                        <ul>\n                            <li class=\"item\">{{$t('footer.advertisement')}}</li>\n                            <li class=\"item\">{{$t('footer.contact')}}</li>\n                        </ul>\n                        <div class=\"form-group col-group\">\n                            <div class=\"form-group row-group box-lang\" v-on:click=\"handle_toogle($event)\">\n                                <li class=\"item-lang\">\n                                    <img :src=\"`${language_selected.icon}`\" :alt=\"`${language_selected.name}`\">\n                                    <span>{{language_selected.name}}</span>\n                                </li>\n                                <i class=\"fas fa-sort-down\"></i>\n                            </div>\n                            <ul class=\"list-lang\" v-if=\"toogle\">\n                                <li class=\"item-lang\" v-for=\"(lang,index) in language\" :key=\"index\" v-on:click=\"handle_choose(lang)\">\n                                    <img :src=\"`${lang.icon}`\" :alt=\"`${lang.name}`\">\n                                    <span>{{lang.name}}</span>\n                                </li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n                </div>\n            </div>\n            <div class=\"footer-bottom\">\n                <div class=\"\" style=\"display: flex; width: 100%; max-width: 1200px; margin: 0 auto; padding: 0 15px; justify-content: space-between; align-item: center;\">\n                <div class=\"social\">\n                    <span class=\"logo-social facebook\" ><a href=\"https://www.facebook.com\" target=\"_blank\"></a></span>\n                    <span class=\"logo-social twitter\" ><a href=\"https://www.twitter.com\" target=\"_blank\"></a></span>\n                    <span class=\"logo-social google\" ><a href=\"https://www.google.com\" target=\"_blank\"></a></span>\n                    <span class=\"logo-social instagram\" ><a href=\"https://www.instagram.com\" target=\"_blank\"></a></span>\n                </div>\n                <div class=\"block copyright\">\n                    <p>Copyright &copy; 2019 Franchise King <i class=\"fas fa-bolt text-color-primarry\"></i> Brave Cat</p>\n                </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<style scoped>\n.box-lang { padding-left: 32px; flex-direction: row; justify-content: space-between; align-items: center;}\n.list-lang { display: flex; flex-direction: column; width: 100%; justify-content: flex-start; align-items: flex-start; padding-left: 32px;}\n.item-lang { display: flex; height: 36px; flex-direction: row; justify-content: center; align-items: center; margin: 4px 0;}\n.item-lang img { width: 36px; height: 100%; margin: 0 8px;}\n</style>\n\n<script>\n\nimport i18n from '@/lang/i18n.js'\nexport default {\n   data() {\n       return {\n           language: [],\n           language_selected: {},\n           toogle: false\n       }\n   },\n   methods: {\n        handle_toogle($event) {\n            $event.stopPropagation();\n            this.toogle = !this.toogle\n        },\n        handle_choose(lang) {\n            this.language_selected = lang\n            this.$store.dispatch('setLang', lang.value)\n        }\n   },\n   mounted() {\n       window.onclick = () => {\n           if(this.toogle) {\n               this.toogle = !this.toogle\n           }\n       }\n   },\n   created() {\n     this.language = [\n        {\n            name: 'Vietnamese',\n            icon: '/img/vi.png',\n            value: 'vn'\n        },\n        {\n            name: 'English',\n            icon: '/img/eng.png',\n            value: 'en'\n        }\n     ]\n    //  this.language_selected = this.language.map(item => {item.value == i18n.locale})\n     this.language.map(item => {\n         if(item.value == i18n.locale) {\n            this.language_selected = item\n        }\n     })\n   }\n}\n</script>"]}]}