            this.url_preview = []\\n                var files = e.target.files || e.dataTransfer.files;\\n                if (!files.length)\\n                    return;\\n                this.qr_code_image = files[0]\\n                this.logo = files[0],\\n                this.images = files[0];\\n                this.thumbnail = files[0]\\n                this.main_banner = files\\n                this.image_intro = files\\n\\n                if(this.is_logo == 'MAIN_BANNER' || this.is_logo == 'IMAGE_INTRO') {\\n                    for(var i = 0; i < files.length; i++) {\\n                        this.url_preview.push(URL.createObjectURL(files[i]))\\n                    }\\n                } else {\\n                    this.url_preview.push(URL.createObjectURL(files[0]));\\n                }\\n\\n                if(this.main_banner.length > 2) {\\n                    alert('Main Banner Only 2 Picture')\\n                    this.main_banner = []\\n                    e.target.value = ''\\n                } else if(this.image_intro.length > 5) {\\n                    alert('Image Intro Only 5 Picture')\\n                    this.image_intro = []\\n                    e.target.value = ''\\n                }\\n                \\n            },\\n            handle_upload() {\\n                switch(this.is_logo) {\\n                    case 'LOGO':\\n                        this.handle_up_logo();\\n                        break;\\n                    case 'THUMBNAIL':\\n                        this.handle_up_thumbnail();\\n                        break;\\n                    case 'MAIN_BANNER':\\n                        this.handle_up_main_banner();\\n                        break;\\n                    case 'IMAGE_INTRO':\\n                        this.handle_up_image_intro();\\n                        break;\\n                    case 'QR_CODE':\\n                        this.handle_up_qr_code_image();\\n                        break;\\n                }\\n   