his2.subjects = result.data.status == 200 ? result.data.data : [];\n        }).catch(function (error) {\n          reject(error);\n          console.log('Error : ' + error);\n        });\n      });\n    },\n    handle_reply_ticket: function handle_reply_ticket() {\n      var _this3 = this;\n\n      this.ticket_selected.history.map(function (item) {\n        if (item.content.length > 0 && item.reply.length == 0) {\n          item.reply = _this3.new_reply;\n        }\n      });\n      var historys = this.ticket_selected.history;\n      return new Promise(function (resolve, reject) {\n        HTTP_ADMIN.patch('ticket-support/reply-ticket/' + _this3.ticket_selected._id, historys).then(function (result) {\n          resolve(result);\n\n          if (result.data.status == 200) {\n            socket_bus.$emit('SHOW_MESSAGE', _this3.$t('reply_success'));\n            _this3.ticket_selected = null;\n          } else {\n            socket_bus.$emit('SHOW_MESSAGE', _this3.$t('reply_fail'));\n          }\n        }).catch(function (error) {\n          reject(error);\n          console.log('Error Update: ' + error);\n        });\n      });\n    }\n  },\n  created: function created() {\n    this.handle_get_tickets();\n    this.handle_get_subject();\n\n    if (localStorage.getItem('lang')) {\n      this.lang_current = localStorage.getItem('lang');\n    } else {\n      this.lang_current = 'vi';\n    }\n  }\n};",{"version":3,"sources":["SupportTicket.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoGA,SAAA,UAAA;AACA,SAAA,UAAA;AAGA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,eAAA,EAAA,EADA;AAEA,MAAA,SAAA,EAAA,KAFA;AAGA,MAAA,eAAA,EAAA,IAHA;AAIA,MAAA,QAAA,EAAA,EAJA;AAKA,MAAA,SAAA,EAAA,EALA;AAMA,MAAA,YAAA,EAAA;AANA,KAAA;AAQA,GAVA;AAWA,EAAA,OAAA,EAAA;AACA,IAAA,uBADA,mCACA,KADA,EACA;AACA,cAAA,KAAA;AACA,aAAA,MAAA;AACA,iBAAA,KAAA,EAAA,CAAA,aAAA,CAAA;;AACA,aAAA,OAAA;AACA,iBAAA,KAAA,EAAA,CAAA,cAAA,CAAA;AAJA;AA