                  </div>\n                    <div class=\"form-content\" style=\"padding: 0;  margin-top: 8px; heigh: auto\">\n                        <div class=\"form-group margin-bottom col-group\"> \n                            <div class=\"form-group\">\n                                <input type=\"text\" :placeholder=\"`${this.$t('form_joining.your_name')}`\" name=\"name\" v-model=\"name\" v-validate=\"'required:true'\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('name')\">{{errors.first('name')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom col-group\"> \n                            <div class=\"form-group\">\n                                <input type=\"text\" :placeholder=\"`${this.$t('form_joining.phone_number')}`\" name=\"phone_number\" v-model=\"phone_number\" v-validate=\"`required: true|regex:^([0-9]+)$`\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('phone_number')\">{{errors.first('phone_number')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom col-group\">\n                            <div class=\"form-group\">\n                                <input type=\"text\" name=\"address\" :placeholder=\"`${this.$t('form_joining.address')}`\" v-model=\"address\" v-validate=\"`required: true`\">\n                            </div>\n                            <span class=\"error-text margin-fix-top\" v-show=\"errors.has('address')\">{{errors.first('address')}}</span>\n                        </div>\n                        <div class=\"form-group margin-bottom col-group\">\n                            <div class=\"form-group\">\n                                <input type=\"text\" name=\"email\" placeholder=\"Email\" v-model=\"email\" v-validate=\"'required:true|email'\">\n         