nimport { HTTP } from \"../../../../http-common\";\nexport default {\n  data: function data() {\n    return {\n      FAQ: [],\n      faq_selected: {},\n      is_detail: false,\n      readonly: true,\n      lang_current: ''\n    };\n  },\n  methods: {\n    handle_translate: function handle_translate(item) {\n      switch (this.lang_current) {\n        case 'en':\n          return item.question_en;\n\n        case 'tw':\n          return item.question_tw;\n\n        case 'vi':\n          return item.question_vi;\n      }\n    },\n    handle_translate_answer: function handle_translate_answer(item) {\n      switch (this.lang_current) {\n        case 'en':\n          return item.answer_en;\n\n        case 'tw':\n          return item.answer_tw;\n\n        case 'vi':\n          return item.answer_vi;\n      }\n    },\n    handle_get_faq: function handle_get_faq() {\n      var _this = this;\n\n      return new Promise(function (resolve, reject) {\n        HTTP.get('/faq').then(function (result) {\n          resolve(result.data);\n          _this.FAQ = result.data.status == 200 ? result.data.data : [];\n        }).catch(function (error) {\n          reject(error);\n          console.log(error);\n        });\n      });\n    },\n    handle_show_detail: function handle_show_detail(item) {\n      this.faq_selected = item;\n      this.is_detail = true;\n    }\n  },\n  created: function created() {\n    this.handle_get_faq();\n\n    if (localStorage.getItem('lang')) {\n      this.lang_current = localStorage.getItem('lang');\n    } else {\n      this.lang_current = 'vi';\n    }\n  }\n};",{"version":3,"sources":["FQA.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+DA,SAAA,IAAA;AAEA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,GAAA,EAAA,EADA;AAEA,MAAA,YAAA,EAAA,EAFA;AAGA,MAAA,SAAA,EAAA,KAHA;AAIA,MAAA,QAAA,EAAA,IAJA;AAKA,MAAA,YAAA,EAAA;AALA,KAAA;AAOA,GATA;AAUA,EAAA,OAAA,EAAA;AACA,IAAA,gBADA,4BACA,IADA,EACA;AACA,cAAA,KAAA,YAAA;AACA,aAAA,I