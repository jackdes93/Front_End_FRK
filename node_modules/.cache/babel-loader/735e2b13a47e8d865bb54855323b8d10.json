password: ''\n      },\n      roles: ['MODERATOR', 'MONITOR']\n    };\n  },\n  methods: {\n    handle_translate_role: function handle_translate_role(role) {\n      switch (role) {\n        case 'MODERATOR':\n          return this.$t('manager.role_mod');\n\n        case 'MONITOR':\n          return this.$t('manager.role_monit');\n      }\n    },\n    handle_add_account: function handle_add_account() {\n      var _this = this;\n\n      HTTP_ADMIN.post('admin', this.account).then(function (result) {\n        if (result.data.status == 201) {\n          _this.account = {\n            full_name: '',\n            user_name: '',\n            roles: '',\n            password: ''\n          };\n          socket_bus.$emit('ADD_ACCOUNT_SUCCESS', {\n            data: result.data.data\n          });\n          socket_bus.$emit('SHOW_ALERT', {\n            is_show: true,\n            message: _this.$t('message_alert.register_account_success'),\n            is_redirect: true\n          });\n          setTimeout(function () {\n            _this.handle_close_form();\n          }, 5000);\n        } else {\n          socket_bus.$emit('SHOW_ALERT', {\n            is_show: true,\n            message: _this.$t('message_alert.register_account_fail'),\n            is_redirect: true\n          });\n        }\n      });\n    },\n    handle_close_form: function handle_close_form() {\n      socket_bus.$emit('CLOSE_FORM_ADD', {\n        data: false\n      });\n    }\n  }\n};",{"version":3,"sources":["FormAddAccount.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6DA,SAAA,UAAA;AACA,SAAA,UAAA;AAEA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,YAAA,EAAA,EADA;AAEA,MAAA,OAAA,EAAA;AAAA,QAAA,SAAA,EAAA,EAAA;AAAA,QAAA,SAAA,EAAA,EAAA;AAAA,QAAA,KAAA,EAAA,EAAA;AAAA,QAAA,QAAA,EAAA;AAAA,OAFA;AAGA,MAAA,KAAA,EAAA,CAAA,WAAA,EAAA,SAAA;AAHA,KAAA;AAKA,GAPA;AAQA,EAAA,OAAA,EAAA;AACA,IAAA,qBADA,iCACA,IADA,EACA;AACA,cAAA,IAAA;AACA,aAAA,WAAA;AACA,iBAAA,KAAA,EAAA,CAAA,kBAAA,CAAA