eak;\n                case 'PREV':\n                    this.i = ( this.i == 0) ? 0 :  this.i - 1\n                    break;\n            }\n            setTimeout(() => {\n                list_brand_element.style.marginLeft = \"-\" + (marginLeft *  this.i) + \"px\";\n            }, 200)\n            \n        },\n        handle_get_top_brand() {\n            return new Promise((resolve, reject) => {\n                HTTP.get(`/top-brand/Category&category=${this.$route.params.category}`).then(result => {\n                    resolve(result.data)\n                    this.top_brand = result.data.status == 200 ? result.data.data : []\n                }).catch(error => {\n                    reject(error)\n                    console.log('Error: ' + error)\n                })\n            })\n        }\n    },\n    created() {\n        this.handle_get_top_brand()\n    }\n}\n</script>"],"sourceRoot":"src/components/Units"}]}