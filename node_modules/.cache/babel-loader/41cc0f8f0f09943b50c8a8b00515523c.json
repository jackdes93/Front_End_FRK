\"font-size: 2.4rem\">Easy to browser your <i class=\"far fa-heart\"></i> brands</h3>\n                </div>\n                <!-- <div class=\"bar-item\">\n                    <div class=\"list-joining\" v-on:click=\"handle_choose('CITY', $event)\">\n                        <span class=\"circle bg-title-header\"><i class=\"fas fa-map-marker-alt\"></i></span>\n                        <p class=\"text-name\">{{city_choose.name}}</p>\n                        <i class=\"fas fa-sort-down\"></i>\n                    </div>\n                    <ul class=\"list-city\" v-if=\"city_show\">\n                        <li class=\"item-city\" v-for=\"(city,i) in list_city\" :key=\"i\" v-on:click=\"handle_select_city(i)\">\n                            <p>{{city.name}}</p>\n                        </li>\n                    </ul>\n                </div>\n                <div class=\"bar-item\">\n                    <div class=\"list-joining not-active\" v-on:click=\"handle_choose('CATEGORY', $event)\">\n                        <span class=\"circle bg-title-header\"><i class=\"fas fa-bookmark\" style=\"color: #FFF\"></i></span>\n                        <p class=\"title\">{{category_choose.name}}</p>\n                        <i class=\"fas fa-sort-down\"></i>\n                    </div> \n                    <ul class=\"list-city\" v-if=\"category_show\" >\n                        <li class=\"item-city\" v-for=\"(category,i) in list_category\" :key=\"i\" v-on:click=\"handle_select_category(i)\">\n                            <p>{{category.name}}</p>\n                        </li>\n                    </ul>\n                </div>-->\n                <div class=\"bar-item padding-left\" style=\"flex: 0.5 !important\">\n                    <div class=\"list-joining not-active\">\n                        <input type=\"text\" style=\"border: 1px solid #FFF\" class=\"title\" id=\"input_keyword\" v-model=\"key_word\"  :placeholder=\"`${this.$t('filter.keyword')}`\" >\n                        <span class=\"circle\" style=\"background: #FFF;\" v-on:click=\"handle_search\"><i class=\"fas fa-filter text-color-primarry\"></i></span>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { HTTP } from '../../http-common';\nimport { router } from '../../routers';\nexport default {\n    data() {\n        return {\n            customer: {},\n            city_show: false,\n            category_show: false,\n            readonly: true,\n            list_city: [],\n            city_choose: {_id: '', name: this.$t('filter.select_city')},\n            list_category: [],\n            category_choose: {_id: '', name: this.$t('filter.choose_category')},\n            key_word: '',\n            search_result:[]\n        }\n    },\n    methods: {\n        handle_choose(state, $event) {\n            $event.stopPropagation();\n            switch(state) {\n                case 'CITY':\n                    this.city_show = !this.city_show\n                    break;\n                case 'CATEGORY':\n                        this.category_show = !this.category_show\n                    break;\n            }\n        },\n        handle_select_city(i) {\n            this.city_choose = this.list_city[i]\n            this.city_show = false;\n        },\n        handle_select_category(i) {\n            this.category_show = false;\n            this.category_choose = this.list_category[i]\n            this.readonly = false\n        },\n        handle_search() {\n           router.push(`/search/category=${this.category_choose.name}&city=${this.city_choose.name}&key=${this.key_word}`)\n        }\n    },\n    mounted() {\n        this.$nextTick(() => {\n            window.addEventListener('click', () => {\n                if(this.city_show) {\n                    this.city_show = false\n                }\n\n                if(this.category_show) {\n                    this.category_show = false\n                }\n            })\n        })\n    },\n    created() {\n        if(JSON.parse(localStorage.getItem('customer'))) {\n            this.customer = JSON.parse(localStorage.getItem('customer'))\n        }\n        this.$store.dispatch('get_cities').then(result => {\n            this.list_city = result.data\n        })\n\n        this.$store.dispatch('get_categories').then(result => {\n            this.list_category = result.data\n        })\n    }\n}\n</script>"],"sourceRoot":"src/components/Units"}]}