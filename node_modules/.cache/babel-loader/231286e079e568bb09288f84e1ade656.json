<div class=\"btn-scroll \">\n            <div class=\"box-scroll bg-title-header\" id=\"box-scroll\"><i class=\"fas fa-angle-up\"></i></div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport block_banner from '../Units/BlockBanner';\nimport block_top_brand from '../Units/BlockTopBrand';\nimport block_nav from '../Units/BlockNav';\nimport block_footer from '../Units/Block_Footer';\nimport Block_Banner_Bottom from '../Units/Block_Banner_Bottom.vue';\nimport { HTTP } from '../../http-common';\n\nexport default {\n    components : {\n        block_banner, block_top_brand, block_nav, block_footer, Block_Banner_Bottom\n    },\n    data() {\n        return {\n            search_result: [],\n            category_choose: '',\n            city_choose: '',\n            key_word: ''\n        }\n    }, \n    methods: {\n        handle_search() {\n            const city = this.city_choose.length > 0 ? this.city_choose : 'null'\n            return new Promise((resolve, reject) => {\n                HTTP.get('/brands/search-brand&category=' + this.category_choose + '&location=' + city + '&key-word=' + this.key_word).then(result => {\n                    resolve(result)\n                    this.search_result = result.data.status == 200 ? result.data.data : []\n                })\n                .catch(error => {\n                    reject(error)\n                    console.log('Error: ' + error)\n                })\n            })\n        }\n    },\n    created() {\n        this.category_choose = this.$route.params.category\n        this.city_choose = this.$route.params.city\n        this.key_word = this.$route.params.key\n        this.handle_search()\n    }    \n}\n</script>"],"sourceRoot":"src/components/Pages"}]}